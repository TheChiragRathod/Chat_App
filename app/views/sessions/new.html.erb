<%= form_with url: login_path,data: { turbo: false } do |form|%>
  <div class="form-group">
    <%= form.label :username %>
    <%= form.email_field :username, class:"form-control", placeholder:"Enter username"%>
  </div>

  <div class="form-group">
    <%= form.label :password %>
    <%= form.password_field :password, class:"form-control", placeholder:"Enter password"%>
  </div>
  <%= form.submit "Submit", class:"btn btn-primary"%>
<% end %>
